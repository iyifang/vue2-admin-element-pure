ipt>\r\nexport default {\r\n  data() {\r\n    return {};\r\n  },\r\n  mounted() {\r\n    // this.fn();\r\n  },\r\n  destroyed() {},\r\n  methods: {\r\n    fn() {\r\n      var eye1 = document.querySelector(\".black1\");\r\n      var eye2 = document.querySelector(\".black2\");\r\n      let x = 0;\r\n      let y = 0;\r\n      let top = 75;\r\n      let left = 75;\r\n\r\n      //添加鼠标移动监听\r\n      window.addEventListener(\"mousemove\", function (e) {\r\n        //获取鼠标到眼睛模块中心的距离\r\n        x =\r\n          e.x - document.querySelector(\".eye\").getBoundingClientRect().x - 200;\r\n        y =\r\n          e.y - document.querySelector(\".eye\").getBoundingClientRect().y - 100;\r\n      });\r\n\r\n      //创建定时器，每10ms图片移动一次\r\n      setInterval(() => {\r\n        if (x > 50) {\r\n          left = 100;\r\n        } else if (x < -50) {\r\n          left = 50;\r\n        } else {\r\n          left = 75;\r\n        }\r\n        if (y > 50) {\r\n          top = 100;\r\n        } else if (y < -50) {\r\n          top = 50;\r\n        } else {\r\n          top = 75;\r\n        }\r\n\r\n        eye1.style.left = left + \"px\";\r\n        eye1.style.top = top + \"px\";\r\n        eye2.style.left = left + \"px\";\r\n        eye2.style.top = top + \"px\";\r\n      }, 10);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\nbody {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  background: #929292;\r\n}\r\n.face {\r\n  width: 250px;\r\n  height: 250px;\r\n  border-radius: 50%;\r\n  background: #e7d90f;\r\n  margin: 50px auto;\r\n}\r\n.eye {\r\n  width: 200px;\r\n  height: 100px;\r\n  margin: auto;\r\n  display: flex;\r\n}\r\n\r\n.white {\r\n  width: 100px;\r\n  height: 100px;\r\n  border-radius: 50%;\r\n  background: #ffffff;\r\n  position: relative;\r\n  margin: auto;\r\n}\r\n.black1 {\r\n  width: 25px;\r\n  height: 25px;\r\n  border-radius: 50%;\r\n  background: #000000;\r\n  position: absolute;\r\n  top: 35px;\r\n  left: 3